<button type="button" class="btn btn-default dropdown-toggle filter" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    {{label}} <span class="caret"></span>
</button>
<ul class="dropdown-menu dropdown-menu-left">
{{#options}}
    <li>
    <div class="{{inputType}} filter">
            <label>
                <input type="{{inputType}}" {{#name}}name="{{name}}" {{/name}}value="{{#id}}{{prefix}}-{{id}}" id="{{prefix}}-{{id}}{{/id}}">
                {{text}}
            </label>
        </div>
    </li>
{{/options}}
</ul> 
